{
  "name": "mychips",
  "version": "0.1.0",
  "description": "Reference MyCHIPs server",
  "main": "lib/mychips.js",
  "preferGlobal": "true",
  "author": "Kyle Bateman",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gotchoices/MyCHIPs/issues"
  },
  "homepage": "https://github.com/gotchoices/MyCHIPs#readme",
  "directories": {
    "dist": "dist",
    "static": "static"
  },
  "bin": {
    "mc_admin": "admin.js",
    "mc_mobi": "mobi.js"
  },
  "scripts": {
    "test": "mocha test/mocha/all.js",
    "test-peercomm": "mocha test/mocha/peercomm.js",
    "test-impexp": "mocha test/mocha/impexp.js",
    "test-peer": "mocha test/mocha/peer.js",
    "dev-hot": "webpack-dev-server --mode development --hot",
    "dev-build": "webpack --mode development --watch --progress --hide-modules",
    "dev-server": "bin/mychips.js --spaPort=0",
    "server": "bin/mychips.js",
    "dev": "npm-run-all --parallel dev-build dev-server",
    "build": "webpack --mode production",
    "start": "bin/mychips.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotchoices/MyCHIPs.git"
  },
  "keywords": [
    "digital",
    "currency",
    "money",
    "bitcoin"
  ],
  "dependencies": {
    "ajv": "^6.2.1",
    "interactjs": "^1.3.3",
    "jquery": "^3.2.1",
    "log4js": "^2.4.1",
    "pg": "^7.4.1",
    "pg-format": "^1.0.4",
    "vue": "^2.5.15",
    "winston": "^3.1.0",
    "ws": "^5.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.8",
    "element-resize-detector": "^1.1.14",
    "file-loader": "^1.1.6",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.8.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.3",
    "vue-loader": "^14.2.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.15",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  }

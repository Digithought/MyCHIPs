{
  "name": "mychips",
  "version": "0.1.9",
  "description": "Reference MyCHIPs server",
  "main": "lib/mychips.js",
  "preferGlobal": "true",
  "author": "Kyle Bateman",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gotchoices/MyCHIPs/issues"
  },
  "homepage": "https://github.com/gotchoices/MyCHIPs#readme",
  "directories": {
    "dist": "dist",
    "static": "static"
  },
  "bin": {
    "mychips": "mychips.js"
  },
  "scripts": {
    "test": "mocha test/mocha/all.js",
    "test-peercomm": "mocha test/mocha/peercomm.js",
    "test-impexp": "mocha test/mocha/impexp.js",
    "test-peer": "mocha test/mocha/peer.js",
    "dev-hot": "webpack-dev-server --mode development --hot --config build/webpack.config",
    "dev-build": "webpack --mode development --watch --progress --hide-modules --config build/webpack.config",
    "build": "webpack --mode production --config build/webpack.config",
    "build-sql": "cd schema; make sql",
    "profile": "webpack --config build/webpack.config --profile --json >/tmp/webpack-stats.json",
    "develop": "bin/develop",
    "dev-server": "bin/mychips.js --spaPort=0",
    "dev": "npm-run-all --parallel dev-build dev-server",
    "server": "bin/mychips.js",
    "start": "bin/localcerts -b spa peer && bin/mychips.js",
    "cert": "wyclif-cert",
    "init": "bin/localcerts spa",
    "adminticket": "bin/adminticket -o test/local/admin.json",
    "agent": "test/sim/agent -m 2",
    "builddb": "cd schema; make objects",
    "preversion": "npm-run-all --parallel build-sql build && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotchoices/MyCHIPs.git"
  },
  "keywords": [
    "digital",
    "crypto",
    "currency",
    "private",
    "credit",
    "money",
    "blockchain",
    "bitcoin"
  ],
  "dependencies": {
    "interactjs": "^1.6.2",
    "jquery": "^3.4.1",
    "json-stable-stringify": "^1.0.1",
    "pg": "^7.12.1",
    "pg-format": "^1.0.4",
    "qr-image": "^3.2.0",
    "string-hash": "^1.1.3",
    "vue": "^2.6.10",
    "winston": "^3.2.1",
    "ws": "^7.2.0",
    "wyclif": "^1.0.8",
    "wylib": "^1.0.20",
    "wyselib": "^1.0.15",
    "wyseman": "^1.0.16",
    "yaml": "^1.8.3",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.2.1",
    "element-resize-detector": "^1.1.15",
    "file-loader": "^5.0.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mocha": "^6.2.2",
    "mongodb": "^3.3.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}

---
mychips.chits:
  fields:
  - {col: chit_ent,	size:	6}
  - {col: chit_seq,	size:	4}
  - {col: chit_idx,	size:	4}
  - {col: units,	size:	10}

mychips.chits_v:
  display: [chit_ent, chit_seq, chit_idx, chit_type, units, value]

mychips.chits_v_me:
  display: [chit_ent, chit_seq, chit_idx, chit_type, units, value]
